{
  "title": "wanshuo-gitbook",
  "description": "记录学习，记录生活",
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    }
  },

  "plugins": ["advanced-emoji", "simple-page-toc", "-lunr", "-search", "search-plus", "github", "github-buttons", "ace"],
  "pluginsConfig": {
    "simple-page-toc": {
		"maxDepth": 3,
		"skipFirstH1": true
	},
	"github": {
        "url": "https://github.com/wansho"
    },
	"github-buttons": {
		"repo": "wansho/wansho-gitbook",
		"types": [
			"star",
			"watch",
			"fork"
		],
		"size": "small"
	}
  }
}
}